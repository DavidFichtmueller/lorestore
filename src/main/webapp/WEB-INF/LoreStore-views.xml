<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-lazy-init="true">
    
    <bean id="ore" class="net.metadata.openannotation.lorestore.views.OREResponseView"/>
    <bean id="oa" class="net.metadata.openannotation.lorestore.views.OANamedGraphsView"/>
    <bean id="sparqlxml" class="net.metadata.openannotation.lorestore.views.SPARQLXMLView"/>
    <bean id="oreAtom" class="net.metadata.openannotation.lorestore.views.OREAtomFeedView" />
    <bean id="oaAtom" class="net.metadata.openannotation.lorestore.views.OAAtomFeedView" />
    <bean id="exception" class="net.metadata.openannotation.lorestore.views.ExceptionView" />
</beans>